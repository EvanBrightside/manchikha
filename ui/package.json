{
  "name": "Manchikha",
  "version": "0.1.0",
  "license": "ISC",
  "description": "Manchikha",
  "readme": "Manchikha",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximderbin/manchikha.git"
  },
  "config": {
    "buildDir": "./build",
    "devHost": "0.0.0.0",
    "devPort": 9000
  },
  "scripts": {
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production npm run webpack -p",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "dependencies": {
    "react": "0.14.3",
    "react-dom": "0.14.3"
  },
  "devDependencies": {
    "autoprefixer-core": "5.2.1",
    "babel-core": "5.8.34",
    "babel-loader": "5.4.0",
    "babel-runtime": "5.8.34",
    "css-loader": "0.14.5",
    "csswring": "3.0.7",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.4",
    "html-loader": "0.3.0",
    "json-loader": "0.5.3",
    "lodash": "^3.10.1",
    "postcss-loader": "0.5.1",
    "react-hot-loader": "1.3.0",
    "style-loader": "0.13.0",
    "template-html-loader": "0.0.3",
    "webpack": "1.12.6",
    "webpack-dev-server": "1.12.1"
  },
  "engines": {
    "node": "4.2.4"
  }
}
